.product-list {
  display: flex;
  width: 100%;
  justify-content: space-between;
  height: 526rpx;
  overflow: hidden;
}

.product-list .left {
  width: 344rpx;
}

.product-list .left image {
  width: 100%;
  height: 100%;
}

.product-list .right {
  width: 400rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.product-list .right image {
  height: 260rpx;
  width: 100%;
}
